<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_create_batch"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/et_create_batch_name_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/textview_border"
        android:inputType="text"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_name"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_name"
        tools:ignore="LabelFor" />

    <Spinner
        android:id="@+id/spinner_createbatch_cotrainer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/spinner_border"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_cotrainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_trainer"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_cotrainer" />

    <Spinner
        android:id="@+id/spinner_createbatch_trainer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/spinner_border"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_trainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_trainer"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_trainer" />

    <Spinner
        android:id="@+id/spinner_createbatch_location"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/spinner_border"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_location"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_location" />

    <Spinner
        android:id="@+id/spinner_create_batch_training_type"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/spinner_border"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_training_type"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_training_type"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_training_type" />

    <EditText
        android:id="@+id/et_create_batch_skill_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/textview_border"
        android:fontFamily="sans-serif"
        android:inputType="text"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_skill"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_skill"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_skill"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/et_create_batch_start_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/textview_border"
        android:fontFamily="sans-serif"
        android:inputType="date"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_start"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_start"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_start"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/et_create_batch_end_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/textview_border"
        android:fontFamily="sans-serif"
        android:inputType="date"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_end"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_end"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_end"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/et_create_batch_good_grade_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/textview_border"
        android:fontFamily="sans-serif"
        android:inputType="number"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_good_grade"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_good_grade"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_good_grade"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/et_create_batch_passing_grade_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:autofillHints=""
        android:background="@drawable/textview_border"
        android:fontFamily="sans-serif"
        android:inputType="number"
        android:paddingStart="4dp"
        android:paddingEnd="1dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_create_batch_passing_grade"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_create_batch_passing_grade"
        app:layout_constraintTop_toTopOf="@+id/tv_create_batch_passing_grade"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/tv_create_batch_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="@string/batch_dialog_heading_name"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_trainer"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_create_batch_trainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/batch_dialog_heading_training_name"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_location"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_name" />

    <TextView
        android:id="@+id/tv_create_batch_cotrainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="CoTrainer Name:"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_location"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_trainer" />

    <TextView
        android:id="@+id/tv_create_batch_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/batch_dialog_heading_location"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_skill"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_training_type" />

    <TextView
        android:id="@+id/tv_create_batch_training_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Training Type:"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_location"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_cotrainer" />

    <TextView
        android:id="@+id/tv_create_batch_skill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/batch_dialog_heading_skill"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_start"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_location" />

    <TextView
        android:id="@+id/tv_create_batch_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/batch_dialog_heading_start_date"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_end"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_skill" />

    <TextView
        android:id="@+id/tv_create_batch_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/batch_dialog_heading_end_date"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_good_grade"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_start" />

    <TextView
        android:id="@+id/tv_create_batch_good_grade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/batch_dialog_heading_good_grade"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_create_batch_passing_grade"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_end" />

    <TextView
        android:id="@+id/tv_create_batch_passing_grade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="20dp"
        android:text="@string/batch_dialog_heading_passing_grade"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_create_batch_good_grade" />

    <Button
        android:id="@+id/btn_create_batch_create"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:background="@drawable/submit_button_style"
        android:text="@string/batch_dialog_create_button"
        android:textColor="@color/icon_plus_background"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_create_batch_cancel" />

    <Button
        android:id="@+id/btn_create_batch_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:background="@drawable/submit_button_style"
        android:text="@android:string/cancel"
        android:textColor="@color/icon_plus_background"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_create_batch_create"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>